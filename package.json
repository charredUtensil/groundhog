{
  "name": "groundhog",
  "version": "0.10.13",
  "homepage": "https://charredutensil.github.io/groundhog",
  "private": true,
  "dependencies": {
    "@stdlib/random-base-beta": "^0.2.1",
    "@stdlib/random-base-mt19937": "^0.2.1",
    "delaunator": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "word-wrap": "^1.2.5"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "presubmit": "bin/presubmit.sh",
    "gen": "tsx bin/groundhog-gen",
    "architect-stats": "tsx bin/architect-stats",
    "find-failures": "VITE_APP_VERSION=test tsx bin/find-failures",
    "lore-dump": "tsx bin/lore-dump",
    "mermaidify": "tsx bin/mermaidify",
    "update-goldens": "UPDATE_GOLDENS=1 react-scripts test --all --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/delaunator": "^5.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "gh-pages": "^6.1.1",
    "prettier": "^3.3.3",
    "sass": "^1.71.1",
    "tsx": "^4.19.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
